{
  "name": "@curium.rocks/socket-io-transceiver",
  "version": "0.1.0",
  "description": "",
  "main": "build/src/lib.js",
  "scripts": {
    "test": "nyc mocha --require ts-node/register --require source-map-support/register --recursive 'test/**/*.ts'",
    "build": "node_modules/typescript/bin/tsc --project ./",
    "lint": "eslint src --ext .ts",
    "clean": "rm -fr build",
    "prepare": "npm run build",
    "doc": "typedoc src/lib.ts",
    "sonarscan": "sonar-scanner -Dsonar.qualitygate.wait=true -Dsonar.organization=curium-rocks -Dsonar.projectKey=curium-rocks_socket-io-transceiver -Dsonar.sources=. -Dsonar.host.url=https://sonarcloud.io -Dsonar.login=$SONARQUBE_KEY -Dsonar.javascript.lcov.reportPath=coverage/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/curim-rocks/socket-io-transceiver.git"
  },
  "keywords": [
    "socket.io",
    "transceiver",
    "transport"
  ],
  "devDependencies": {
    "@curium.rocks/ping-pong-emitter": "^0.1.1-15",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "chai": "^4.3.4",
    "eslint": "^7.30.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^9.0.2",
    "nyc": "^15.1.0",
    "sonarqube-scanner": "^2.8.1",
    "ts-node": "^10.1.0",
    "typedoc": "^0.21.4",
    "typescript": "^4.3.5"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "lcov"
    ],
    "all": true
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/curium-rocks/socket-io-transceiver/issues"
  },
  "homepage": "https://github.com/curium-rocks/socket-io-transceiver#readme",
  "dependencies": {
    "@curium.rocks/data-emitter-base": "^0.1.1-alpha.41",
    "socket.io": "^4.4.0",
    "socket.io-msgpack-parser": "^3.0.1"
  }
}
